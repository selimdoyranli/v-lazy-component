{
  "name": "v-lazy-component",
  "version": "3.0.0",
  "description": "Vue component render when visible",
  "keywords": [
    "vue lazy component",
    "vue lazy",
    "vue2 lazy",
    "vue3 lazy"
  ],
  "author": "selimdoyranli",
  "contributors": [
    "Selim DoyranlÄ± <selimdoyranli@gmail.com> (https://selimdoyranli.com)"
  ],
  "bugs": "selimdoyranli@gmail.com",
  "homepage": "https://v-lazy-component.now.sh",
  "repository": {
    "type": "git",
    "url": "https://github.com/RadKod/v-lazy-component"
  },
  "main": "dist/vue3/index.js",
  "module": "dist/vue3/v-lazy-component.mjs",
  "browser": "dist/vue3/v-lazy-component.global.js",
  "unpkg": "dist/vue3/v-lazy-component.global.js",
  "exports": {
    ".": {
      "require": "./dist/vue3/v-lazy-component.cjs",
      "import": "./dist/vue3/index.js",
      "browser": "./dist/vue3/v-lazy-component.global.js",
      "umd": "./dist/vue3/v-lazy-component.umd.js",
      "unpkg": "./dist/vue3/v-lazy-component.global.js",
      "module": "./dist/vue3/v-lazy-component.mjs"
    },
    "./vue2": "./dist/vue2/index.js"
  },
  "files": [
    "dist",
    "vue2",
    "meta"
  ],
  "sideEffects": false,
  "scripts": {
    "build:vue3": "cross-env NODE_ENV=production rollup --config build/rollup.config.vue3.js",
    "build:ssr:vue3": "cross-env NODE_ENV=production rollup --config build/rollup.config.vue3.js --format cjs",
    "build:es:vue3": "cross-env NODE_ENV=production rollup --config build/rollup.config.vue3.js --format es",
    "build:unpkg:vue3": "cross-env NODE_ENV=production rollup --config build/rollup.config.vue3.js --format iife",
    "build:vue2": "cross-env NODE_ENV=production rollup --config build/rollup.config.vue2.js",
    "build:ssr:vue2": "cross-env NODE_ENV=production rollup --config build/rollup.config.vue2.js --format cjs",
    "build:es:vue2": "cross-env NODE_ENV=production rollup --config build/rollup.config.vue2.js --format es",
    "build:unpkg:vue2": "cross-env NODE_ENV=production rollup --config build/rollup.config.vue2.js --format iife",
    "build": "yarn build:vue3 && yarn build:vue2",
    "commit": "cz",
    "changelog": "changelogen"
  },
  "lint-staged": {},
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-transform-modules-umd": "^7.12.1",
    "@babel/preset-env": "^7.12.10",
    "@babel/preset-typescript": "^7.21.5",
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^24.1.0",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rollup/plugin-replace": "^5.0.2",
    "@vue/compiler-sfc": "^3.0.4",
    "autoprefixer": "^10.1.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-plugin-rename-umd-globals": "^1.0.0",
    "changelogen": "^0.5.3",
    "commitizen": "^4.3.0",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "flush-promises": "^1.0.2",
    "husky": "4.2.5",
    "lint-staged": "^13.2.2",
    "node-sass": "^7.0.0",
    "postcss": "^8.2.1",
    "rollup": "^2.34.2",
    "rollup-plugin-css-only": "2.0.0",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^4.1.2",
    "vue-next": "npm:vue@^3.2.20",
    "vue-next-rollup-plugin-vue": "npm:rollup-plugin-vue@^6.0.0",
    "vue-prev": "npm:vue@^2.6.14",
    "vue-prev-composition-api": "npm:@vue/composition-api@^1.2.4",
    "vue-prev-rollup-plugin-vue": "npm:rollup-plugin-vue@^5.1.9",
    "vue-template-compiler": "^2.6.14"
  },
  "engines": {
    "node": ">=12"
  },
  "license": "MIT",
